{
  "buildCommand": "bundle install && bundle exec jekyll build",
  "outputDirectory": "_site",
  "framework": null,
  "installCommand": "gem install bundler",
  "devCommand": "bundle exec jekyll serve --port $PORT",
  "cleanUrls": false,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/sitemap.xml",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/xml; charset=utf-8"
        }
      ]
    },
    {
      "source": "/feed.xml",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/xml; charset=utf-8"
        }
      ]
    }
  ]
}
